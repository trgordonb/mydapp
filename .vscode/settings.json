{
  "workbench.colorTheme": "Default Light+",
  "window.zoomLevel": 0,
  "eslint.enable": true,
  "editor.tabSize": 2,
  "cSpell.userWords": [
    "antd",
    "ethereum",
    "kovan",
    "mainnet",
    "metamask",
    "polymathnetwork",
    "predeploy",
    "tokenholders"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
}
